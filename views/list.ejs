<%-include('header'); -%>
 <div class="box" id="heading">
 <h1><%=listTittle%></h1>
</div>

<form action="/delete" method="post">
<div class="box">
  <% newItem.forEach(function(e){ %>
   <div class="item">
     <input type="checkbox" name="checkbox" value="<%=e._id %>" onchange="this.form.submit()">
    <p><%=e.name%></p>
  </div>
<% }); %>
</form>

<form action="/" method="post" class="item ">
  <input type="text" name="newItem" id="" placeholder="New Item" autocomplete="off">
    <button type="submit" name="list" value=<%=listTittle%>>+</button>
     </form>

</div>
<%-include('footer'); -%>

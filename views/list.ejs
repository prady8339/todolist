<%-include('header'); -%>
 <div class="box" id="heading">
 <h1><%=listTittle%></h1>
</div>
<div class="box">
  <% for(var i=0;i<newItem.length;i++){ %>
   <div class="item">
     <input type="checkbox">
    <p><%=newItem[i]%></p>
  </div>
<% } %>

<form action="/" method="post" class="item ">
  <input type="text" name="newItem" id="" placeholder="New Item" autocomplete="off">
    <button type="submit" name="list" value=<%=listTittle%>>+</button>
     </form>

</div>
<%-include('footer'); -%>
